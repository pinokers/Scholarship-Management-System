<main>
	<%- include('../partials/adminsidebar') %>
  <link rel="stylesheet" href="/public/css/admin-edit-schedule.css">
  <div id="main-wrapper">
    <div class="container edit-schedule">
<div class="bg-white shadow-sm p-3 ">
  <span class="me-3" id="sidebar-toggler-btn"><i class="fas fa-bars"></i></span>
  <h5 class="m-0 color-theme d-inline-block">Edit Schedule</h5>
</div>
<div class="col py-2">
    <div class="row">
      <div class="col">
        <nav aria-label="breadcrumb">
          <ol class="breadcrumb">
            <li class="breadcrumb-item"><a href="/admin/admin-pre-reg-schedule">Create Schedule</a></li>
            <li class="breadcrumb-item active">Edit</li>
          </ol>
        </nav>
      </div>
      <div class="col text-end fw-lighter">
        <b>Last Updated <%= new Date(schedule.updatedAt).toLocaleString() %> </b>
      </div>
    </div>
  </div>

  <h2 class=" text-center py-3 edit-schedule-application-header">Edit Schedule</h2>

<div class="container application-schedule-edit-container">
 
  <form action="/admin/admin-pre-reg-schedule-edit/<%= schedule._id %>?_method=PUT" method="POST">
      
    <div class="form-group">
        <label for="start_date">Start Date:</label>
        <input type="datetime-local" class="form-control" id="start_date" name="startDate" value="<%= formatToLocalDatetime(schedule.startDate) %>">
    </div>
    <div class="form-group">
        <label for="end_date">End Date:</label>
        <input type="datetime-local" class="form-control" id="end_date" name="endDate" value="<%= formatToLocalDatetime(schedule.endDate) %>">
    </div>
    
      <button type="submit" class="btn btn-primary">Save Changes</button>
    </form>
  </div>
    
  </div>
</div>
   
</main>